$mapfile_version 2

SYMBOL_VERSION JEFFPC_0.10 {
	protected:
		# error
		cmn_err;
		cmn_verr;
		jeffpc_assfail3;
		jeffpc_assfail;
		jeffpc_log;
		jeffpc_print;
