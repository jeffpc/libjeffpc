		# init
		jeffpc_init;

		# io
		read_file_common;
		write_file;
		xread;
		xwrite;

		# padding
		check_padding;

		# rand
		rand32;
		rand64;

		# sexpr
		sexpr_alist_lookup_int;
		sexpr_alist_lookup_list;
		sexpr_alist_lookup_str;
		sexpr_alist_lookup_val;
		sexpr_array_to_list;
		sexpr_assoc;
		sexpr_car;
		sexpr_cdr;
		sexpr_dump;
		sexpr_dump_file;
		sexpr_equal;
		sexpr_list_to_array;
		sexpr_parse;

		# str
		str_alloc;
		str_cat;
		str_cmp;
		str_dup;
		str_free;
		str_getref;
		str_len;
		str_putref;

		# uuid
		xuuid_clear;
		xuuid_compare;
		xuuid_generate;

		# val
		val_alloc;
		val_dump;
		val_free;
		val_set_bool;
		val_set_cons;
		val_set_int;
		val_set_str;
		val_set_sym;

	local:
		*;
};
